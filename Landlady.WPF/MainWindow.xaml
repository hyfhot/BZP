<Window x:Class="Landlady.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="包租婆" Height="500" Width="600" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="69*"/>
            <ColumnDefinition Width="226*"/>
            <ColumnDefinition Width="297*"/>
        </Grid.ColumnDefinitions>
        <TreeView Height="100" Margin="3,0,0,335" VerticalAlignment="Bottom" Grid.ColumnSpan="2" HorizontalAlignment="Left" Width="100" RenderTransformOrigin="0.5,0.5">
            <TreeView.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-0.562"/>
                    <RotateTransform/>
                    <TranslateTransform X="0.49"/>
                </TransformGroup>
            </TreeView.RenderTransform>
        </TreeView>

    </Grid>
</Window>
